import java.util.*;
public class print {
	Scanner sc=new Scanner(System.in);
	System.out.println("Enter value of n: ");
	public int n=sc.nextInt();	
	public void prime()
	{
		System.out.println("Enter value of n: ");
		int n=sc.nextInt();	
		System.out.println("Prime num from 1 to n: ");
		for(int i=2;i<=n;i++)
		{
			int count=0;
			for(int num=1;num<=i;num++)
			{
				if(i%num==0)
					count++;
			}
			if(count==2)
				System.out.println(i + " ");
		}
	}
	public void even()
	{
		System.out.println("Enter value of n: ");
		int n=sc.nextInt();	
		System.out.println("Even num from 1 to n: ");
		for(int i=1;i<=n;i++)
		{
			if(i%2==0)
				System.out.println(i);
		}
	}
	public void odd()
	{
		System.out.println("Enter value of n: ");
		int n=sc.nextInt();	
		System.out.println("Odd num from 1 to n: ");
		for(int i=1;i<=n;i++)
		{
			if(i%2!=0)
				System.out.println(i);
		}
	}
public static void main(String args[])
{
		new print();
}
}

